table 'Player Comparison'
	lineageTag: c87ff88f-0f68-4c62-a154-d813a6e561d8

	calculationGroup
		precedence: 1

		calculationItem 'Total Points' =
				IF(
				    ISSELECTEDMEASURE(
				        [Selected M]
				    ), [Total pts by GW],
				    SELECTEDMEASURE()
				)

		calculationItem 'Pts Running' = ```
				IF(
				    [Total pts by GW] = BLANK(), BLANK(),
				    
				    IF(
				        ISSELECTEDMEASURE(
				            [Selected M]
				        ),
				        
				        
				        
				        CALCULATE(
				            [Total pts by GW],
				            FILTER(
				                ALL( 'Player by GW'[round] ),
				            'Player by GW'[round] <= MAX( 'Player by GW'[round] ) ) ),
				            SELECTEDMEASURE()
				        )
				    )
				```

		calculationItem Bonus =
				IF(
				    ISSELECTEDMEASURE(
				        [Selected M]
				    ),
				    [Bonus],
				    SELECTEDMEASURE()
				)

	measure 'Selected M' = ```
			IF( SELECTEDMEASURE() = [Hide Filter], [Hide Filter], SELECTEDMEASURE() )

			```
